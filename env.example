# Google Gemini API
GOOGLE_API_KEY=your_gemini_api_key_here

# PostgreSQL Database (either provide DATABASE_URL or individual params)
DATABASE_URL=
POSTGRES_USER=postgres
POSTGRES_HOST=localhost
POSTGRES_DB=rag_database
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_PORT=5432

# Qdrant Vector Database
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# Multi-Agent System Configuration
# Domain-specific models (can be different models for different domains)
HEALTH_MODEL=gemini-1.5-flash
AGRICULTURE_MODEL=gemini-1.5-flash
LEGAL_MODEL=gemini-1.5-flash
FINANCE_MODEL=gemini-1.5-flash
EDUCATION_MODEL=gemini-1.5-flash
GENERAL_MODEL=gemini-1.5-flash

# MCP Server Configuration
MCP_SERVER_ENABLED=true
MCP_SERVER_URL=http://localhost:3001
MCP_SERVER_API_KEY=your_mcp_server_api_key_here

# Web Search Configuration
WEB_SEARCH_ENABLED=true
WEB_SEARCH_API_KEY=your_web_search_api_key_here

# Optional: Google Custom Search API (for enhanced web search)
GOOGLE_CUSTOM_SEARCH_API_KEY=your_google_custom_search_api_key_here
GOOGLE_CUSTOM_SEARCH_ENGINE_ID=your_google_custom_search_engine_id_here

# Optional: Bing Web Search API (for enhanced web search)
BING_SEARCH_API_KEY=your_bing_search_api_key_here

# CORS / Frontend origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000

# Next.js (if applicable)
NEXT_PUBLIC_API_URL=http://localhost:8000